{
  "cli": {
    "version": ">= 16.19.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "channel": "development",
      "distribution": "internal",
      "developmentClient": true,
      "env": {
        "EXPO_PUBLIC_APP_ENV": "preview",
        "NODE_OPTIONS": "--max-old-space-size=4096"
      },
      "cache": {
        "disabled": true
      },
    },
    "preview": {
      "channel": "preview",
      "distribution": "internal",
      "developmentClient": true,
      "env": {
        "EXPO_PUBLIC_APP_ENV": "preview"
      }
    },
    "testing": {
      "channel": "testing",
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_APP_ENV": "testing"
      }
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "env": {
        "EXPO_PUBLIC_APP_ENV": "production"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
